{
  "kind": "collectionType",
  "collectionName": "sn_profiles",
  "info": {
    "singularName": "sn-profile",
    "pluralName": "sn-profiles",
    "displayName": "SN_Profile"
  },
  "options": {
    "draftAndPublish": true,
    "privateAttributes": [
      "updatedAt",
      "meta"
    ]
  },
  "pluginOptions": {},
  "attributes": {
    "fullName": {
      "type": "string"
    },
    "username": {
      "type": "string"
    },
    "age": {
      "type": "integer"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "M",
        "F"
      ]
    },
    "phone": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "address": {
      "type": "json"
    },
    "picture": {
      "type": "string"
    }
  }
}
